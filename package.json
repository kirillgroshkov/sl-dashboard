    "bootstrap": "^4.3.1",
    "chart.js": "^2.9.2"
  },
  "devDependencies": {
    "@naturalcycles/dev-lib": "^9.0.3",
    "@stencil/dev-server": "^0.0.18",
    "@stencil/sass": "^1.1.1",
    "@stencil/utils": "0.0.5",
    "@types/chart.js": "^2.8.11",
    "@types/node": "^12.12.7"
